defaults
auth on
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
protocol smtp
logfile ~/.local/share/msmtp/msmtp.log

account me@tomaskala.com
host smtp.fastmail.com
port 465
tls_starttls off
from me@tomaskala.com
user tomaskala@fastmail.com
passwordeval "pass communication/me@tomaskala.com_imap_pc"
